{
    "compilerOptions": {
        "target": "es5",
        "removeComments": true,
        "preserveConstEnums": true,
        "outFile": "./build/physics.js",
        "sourceRoot": "./src/",
        "rootDir": "./src/",
        "sourceMap": true,
        "noImplicitAny": true,
        "noImplicitReturns": true,
        "noImplicitThis": true,
        "module": "none",
        "alwaysStrict": true,
        "strict": true,
        "strictNullChecks": true,
    }
}